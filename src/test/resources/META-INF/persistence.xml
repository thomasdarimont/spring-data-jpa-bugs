<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="default">
		<class>org.springframework.data.jpa.bugs.domain.User</class>
		<class>org.springframework.data.jpa.bugs.domain.Role</class>
		<class>org.springframework.data.jpa.bugs.domain.Employee</class>
		<class>org.springframework.data.jpa.bugs.domain.Department</class>
		<class>org.springframework.data.jpa.bugs.domain.Person</class>
		<class>org.springframework.data.jpa.bugs.domain.Address</class>
		<exclude-unlisted-classes>true</exclude-unlisted-classes>
	</persistence-unit>
</persistence>
